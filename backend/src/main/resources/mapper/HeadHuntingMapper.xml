<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.green.backend.repository.dao.headhunting.HeadHuntingDao">
    <select id="getStackFilters" resultType="org.green.backend.dto.headhunting.StackDto">
        SELECT cd, cd_nm, up_cd
        FROM tbl_codeinfo
        WHERE up_cd = 'stack_cd'
    </select>
    
    <select id="getCareerFilters" resultType="org.green.backend.dto.headhunting.CareerDto">
        SELECT cd, cd_nm
        FROM tbl_codeinfo
        WHERE up_cd = 'job_notice_career_gbn_cd'
    </select>
</mapper>